#include <stdio.h>
int main()
{
  int a,b,c;
  printf("enter 3 numbers\n");
  scanf("%d%d%d",&a,&b,&c);
  if((a+b)>=c)
  {
      if(a==b==c)
      printf("equilateral");
     else if((a==b)||(b==c)||(a==c))
          printf("isosceles");
          else
            printf("scalene");
  }
   else
  printf("invalid triangle");
}
