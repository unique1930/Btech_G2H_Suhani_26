#include <stdio.h>
#include<math.h>
int main()
{
  int a,b,c,r1,r2,x;
  printf("enter 3 numbers\n");
  scanf("%d%d%d",&a,&b,&c);
  r1=(-b+sqrt((b*b)-4*a*c))/(2*a);
  r2=(-b-sqrt((b*b)-4*a*c))/(2*a);
 printf(" first root of quadratic equation is %d\n",r1);
 printf(" second root of quadratic equation is %d",r2);
 
}
